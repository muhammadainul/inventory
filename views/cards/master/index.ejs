<div class="card">
    <div class="card-body">
        <div class="col-lg-12">
            <div class="alert alert-info">
                <button class="close" data-dismiss="alert">
                    <i class="ace-icon fa fa-times"></i>
                </button>

                <i class="ace-icon fa fa-hand-o-right"></i>
                Pastikan memilih rentan tanggal jika ingin filter & export data.
            </div>
            <form class="form-horizontal" method="post" action="/master_data/export">
                <div class="row mb-5">
                    <div class="col-md-4">
                        Dari <input required type="datetime-local" name="start_date_barang"
                        placeholder="Bulan" class="form-control" id="start_date_barang">
                    </div>
                    <div class="col-md-4">
                        Sampai <input required type="datetime-local" name="end_date_barang"
                         placeholder="Tahun" class="form-control" id="end_date_barang"> 
                    </div>
                    <div class="col-md-4">
                        <span>&nbsp;</span>
                        <select class="form-control" id="kategori_id">
                            <option value="">Kategori</option>
                            <% kategori.forEach(row => { %>
                                <option value="<%= row.id %>"><%= row.kategori %></option>
                            <% }) %>
                        </select>
                    </div>
                </div> 
                <span>&nbsp;</span>
                <div class="text-right clearfix">
                    <button class="btn btn-secondary btn-sm btn-bold" type="reset">
                        <i class="ace-icon fa fa-undo bigger-110"></i>
                        Reset
                    </button>

                    <button type="button" class="btn btn-primary btn-sm btn-bold" id="buttonFilterMasterBarang">
                    <i class="fa fa-search"></i> Filter</button>

                    <button class="btn btn-danger btn-sm btn-bold" type="submit">
                        <i class="ace-icon fa fa-download bigger-110"></i>
                        Export
                    </button>
                </div>
            </form>
        </div>
    </div>
</div>

<div class="table-header">
    <!-- Results for "Latest Registered Domains" -->
</div>

<a href="/master_data/tambah" class="btn btn-white btn-bold btn-primary">
    <i class="ace-icon fa fa-plus"></i> Barang
</a>
<p>&nbsp;</p>
<div>
    <table id="barangTable" class="table table-striped table-bordered table-hover">
        <thead>
            <tr>
                <th class="center">No</th>
                <th>Kode Barang</th>
                <th>Nama Barang</th>
                <th>Harga Beli</th>
                <th>Harga Jual</th>
                <th>Keterangan</th>
                <th>Kategori</th>
                <th>Gambar</th>
                <th>Aksi</th>
            </tr>
        </thead>
    </table>
</div>